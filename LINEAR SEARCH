#include<stdio.h>
int main(){
	int n,arr[100];
	int temp,d,s;
	int found=0,ind=-1;
	
	printf("Enter the number of number of elements:- ");
	scanf("%d",&n);
	for(int i=0;i<n;i++){
		printf("Enter the element:- ");
		scanf("%d",&arr[i]);
	}
	for(int i=0;i<n;i++){
		printf("%d",arr[i]);
	}
printf("Enter the element to search:- ");
	scanf("%d",&s);
	for(int i=0;i<n;i++){
		if(arr[i]==s){
			found=1;
			ind=i;
			break;
		}
	}
	if(found){
		printf("Element found:- %d",ind);
	}
	else{
		printf("Element not found");
	}
printf("\n enter the display element:");
scanf("%d",&d);
printf("displayed element:%d",arr[d]);
}
