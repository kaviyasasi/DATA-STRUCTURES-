#include<stdio.h>
int main(){
	int n,arr[100];
	printf("Enter the number of elements:- ");
	scanf("%d",&n);
	for(int i=0;i<n;i++){
		printf("Enter the element:- ");
		scanf("%d",&arr[i]);
	}
	printf("ELEMENTS\n");
	for(int i=0;i<n;i++){
		printf("%d\n",arr[i]);
	}
	printf("REVERSE OF ELEMENTS");
	for(int i=n;i>=0;i--){
		printf("\n %d",arr[i]);
	}
}
